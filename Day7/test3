<script id="jsbin-javascript">
var name = "arief";
var age = 15;
var money = 500000;
if(name === "") {
  console.log("anda tidak boleh masuk")
} else if(age <17){
  console.log("nama "+name +" umur "+age+" pesan jus")
  if(money < 50000){
    console.log("uang kurang")
  } else {
    money -= 50000
    console.log("kembali "+money)
  }
} else if(age > 17){
  console.log("nama"+name +" umur"+age+" pesan anggur")
  if(money < 300000){
    console.log("uang kurang")
  } else {
    money -= 300000
    console.log("kembali "+money)
   }
}
</script>
